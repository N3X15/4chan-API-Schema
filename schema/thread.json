{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "thread-entity#",
  "title": "Thread entity",
  "description": "A post which also describes information about the thread it spearheads.",
  "type": "object",
  "required": [
    "no",
    "resto",
    "now",
    "time",
    "tim"
  ],
  "properties": {
    "no": {
      "id": "no",
      "type": "number"
    },
    "resto": {
      "id": "resto",
      "type": "number"
    },
    "sticky": {
      "id": "sticky",
      "type": "number"
    },
    "closed": {
      "id": "closed",
      "type": "number"
    },
    "archived": {
      "id": "archived",
      "type": "number"
    },
    "now": {
      "id": "now",
      "type": "string"
    },
    "time": {
      "id": "time",
      "type": "number"
    },
    "name": {
      "id": "name",
      "type": "string"
    },
    "trip": {
      "id": "trip",
      "type": "string"
    },
    "id": {
      "id": "id",
      "type": "string"
    },
    "capcode": {
      "id": "capcode",
      "type": "string"
    },
    "country": {
      "id": "country",
      "type": "string"
    },
    "country_name": {
      "id": "country_name",
      "type": "string"
    },
    "sub": {
      "id": "sub",
      "type": "string"
    },
    "com": {
      "id": "com",
      "type": "string"
    },
    "tim": {
      "id": "tim",
      "type": "number"
    },
    "filename": {
      "id": "filename",
      "type": "string"
    },
    "ext": {
      "id": "ext",
      "type": "string"
    },
    "fsize": {
      "id": "fsize",
      "type": "number"
    },
    "md5": {
      "id": "md5",
      "type": "string"
    },
    "w": {
      "id": "w",
      "type": "number"
    },
    "h": {
      "id": "h",
      "type": "number"
    },
    "tn_w": {
      "id": "tn_w",
      "type": "number"
    },
    "tn_h": {
      "id": "tn_h",
      "type": "number"
    },
    "filedeleted": {
      "id": "filedeleted",
      "type": "number"
    },
    "spoiler": {
      "id": "spoiler",
      "type": "number"
    },
    "custom_spoiler": {
      "id": "custom_spoiler",
      "type": "number",
      "minimum": 1,
      "maximum": 99
    },
    "omitted_posts": {
      "id": "omitted_posts",
      "type": "number"
    },
    "omitted_images": {
      "id": "omitted_images",
      "type": "number"
    },
    "replies": {
      "id": "replies",
      "type": "number"
    },
    "images": {
      "id": "images",
      "type": "number"
    },
    "bumplimit": {
      "id": "bumplimit",
      "type": "number"
    },
    "imagelimit": {
      "id": "imagelimit",
      "type": "number"
    },
    "capcode_replies": {
      "id": "capcode_replies",
      "type": "object",
      "additionalProperties": {
        "id": "capcode_reply_list",
        "type": "array",
        "items": {
          "id": "capcode_reply_ids",
          "type": "number"
        }
      }
    },
    "last_modified": {
      "id": "last_modified",
      "type": "number"
    },
    "tag": {
      "id": "tag",
      "type": "string"
    },
    "semantic_url": {
      "id": "semantic_url",
      "type": "string"
    }
  }
}
